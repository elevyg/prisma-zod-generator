{
  "name": "@elevyg/prisma-zod-enums-generator",
  "version": "0.0.12",
  "description": "Prisma 2+ generator to emit Zod Enums schemas from your Prisma schema",
  "repository": "https://github.com/elevyg/prisma-zod-generator",
  "bin": {
    "prisma-zod-generator": "lib/generator.js"
  },
