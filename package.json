  },
  "keywords": [
    "prisma",
    "prisma-client",
    "prisma-schema",
    "zod",
    "prisma-generator",
    "prisma-zod-generator",
    "enums"
  ],
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^4.3.1",
    "@prisma/generator-helper": "^4.3.1",
    "@prisma/internals": "^4.3.1",
    "prettier": "^2.7.1",
    "ts-morph": "^16.0.0",
    "tslib": "^2.4.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@types/node": "^18.7.18",
    "@types/prettier": "^2.7.0",
    "prisma": "^4.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "homepage": "https://github.com/elevyg/prisma-zod-generator#readme",
